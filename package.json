{
  "name": "airquality-measurements",
  "version": "0.0.1",
  "description": "Implements an API on top of GraphQL to enable storing of airquality measurements",
  "engines": {
    "node": ">= 10.13 <11"
  },
  "devDependencies": {
    "@types/jest": "^24.0.19",
    "@types/node": "^13.7.4",
    "jest": "^24.9.0",
    "prettier": "~1.18.2",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.1.0",
    "tsc-watch": "^4.0.0",
    "tslint": "~5.19.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-microsoft-contrib": "~6.2.0",
    "tsutils": "~3.17.0",
    "typescript": "^3.6.4"
  },
  "scripts": {
    "start": "yarn run build && node build/src/server.js",
    "dev": "nodemon './src/server.ts' --exec ts-node",
    "clean": "rimraf coverage build tmp",
    "build": "tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "lint": "tslint -t stylish --project \"tsconfig.json\"",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "author": "Feetu Nyrhinen <feetu.nyrhinen@gmail.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "@types/express": "^4.17.2",
    "apollo-server": "^2.10.1",
    "bcrypt": "^4.0.0",
    "body-parser": "^1.19.0",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-autosanitizer": "^1.0.1",
    "express-sanitizer": "^1.0.5",
    "express-validator": "^6.2.0",
    "graphql": "^14.6.0",
    "graphql-tools": "^4.0.7",
    "helmet": "^3.21.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.3.2",
    "mongoose": "^5.9.2",
    "nodemon": "^2.0.2",
    "npm-watch": "^0.6.0",
    "saslprep": "^1.0.3",
    "strip-js": "^1.2.0",
    "ts-node": "^8.4.1",
    "tslib": "~1.10.0",
    "winston": "^3.2.1"
  }
}
