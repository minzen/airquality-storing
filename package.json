    "express": "^4.17.1",
    "express-autosanitizer": "^1.0.1",
    "express-sanitizer": "^1.0.5",
    "express-validator": "^6.2.0",
    "graphql": "^14.6.0",
    "graphql-tools": "^4.0.7",
    "helmet": "^5.1.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.3.2",
    "mongoose": "^5.13.15",
